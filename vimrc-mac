set nocompatible
filetype plugin on
syntax enable
set laststatus=2
set statusline=%<%f\%h%m%r%=%-20.(line=%l\ \ col=%c%V\ \ totlin=%L%)\ \ \%h%m%r%=%-40(bytval=0x%B,%n%Y%)\%P
set ofu=syntaxcomplete#Complete
set paste
set backspace=indent,eol,start
set nohlsearch
set incsearch
set ignorecase
set ruler
set expandtab
